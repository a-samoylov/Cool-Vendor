        <api_v1>
            <version>1.0</version>
            <list_commands>
                <command1>
                    <name>GetLastProducts</name>
                    <handler>GetProductsHandler</handler>
                    <validators>
                        <validator>GetProductsValidator</validator>
                    </validators>
                    <properties>

                    </properties>
                </command1>

                <command2>
                    <name>GetProducts22</name>
                    <handler>GetProductHandler</handler>
                    <validators>
                        <validator>GetProductsValidator</validator>
                    </validators>
                    <properties>

                    </properties>
                </command2>
            </list_commands>
        </api_v1>

        <api_v2>
            <version>2.0</version>
            <list_commands>
                <command2>
                    <name>GetProducts</name>
                    <handler>GetProductsHandler</handler>
                    <validators>
                        <validator1>GetProductsValidator</validator1>
                        <validator2>GetProductsValidator</validator2>
                    </validators>
                    <properties>

                    </properties>
                </command2>
            </list_commands>
        </api_v2>
